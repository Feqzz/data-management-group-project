<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xml:base="http://tisk.ml/ontology/parking"
         xmlns="http://tisk.ml/ontology/parking#">

<owl:Ontology rdf:about="http://tisk.ml/ontology/parking"/>

<owl:DatatypeProperty rdf:about="#active">
  <rdfs:domain rdf:resource="#ParkingFacility"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#boolean"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#deactivation_date">
  <rdfs:domain rdf:resource="#ParkingFacility"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#activation_date">
  <rdfs:domain rdf:resource="#ParkingFacility"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#dateTime"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#handicap_information">
  <rdfs:domain rdf:resource="#ParkingFacility"/>
  <rdfs:range rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#PlainLiteral"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#no_of_handicap_parking_spaces">
  <rdfs:domain rdf:resource="#ParkingFacility"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#no_of_electric_vehicle_chargers">
  <rdfs:domain rdf:resource="#ParkingFacility"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#no_of_parking_spaces_with_fee">
  <rdfs:domain rdf:resource="#ParkingFacility"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#no_of_parking_spaces_without_fee">
  <rdfs:domain rdf:resource="#ParkingFacility"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#integer"/>
</owl:DatatypeProperty>

<owl:Class rdf:about="#ParkingCompany">
  <rdfs:subClassOf rdf:resource="http://schema.mobivoc.org/#ParkingFacility"/>
</owl:Class>

<owl:Class rdf:about="#ParkingFacility">
  <rdfs:subClassOf rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
</owl:Class>

<owl:Class rdf:about="#ParkingLot">
  <rdfs:subClassOf rdf:resource="#ParkingFacility"/>
</owl:Class>

<owl:Class rdf:about="#StreetParking">
  <rdfs:subClassOf rdf:resource="#ParkingFacility"/>
</owl:Class>

<owl:Class rdf:about="#ParkingGarage">
  <rdfs:subClassOf rdf:resource="#ParkingFacility"/>
</owl:Class>

<owl:Class rdf:about="#is_operated_by">
  <rdfs:subClassOf rdf:resource="#ParkingFacility"/>
</owl:Class>

</rdf:RDF>
